    "sonner": "^1.7.2",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.0.3",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "watch-sass": "sass --watch src/assets/sass/index.scss:src/assets/css/styles.css",
    "eject": "react-scripts eject",
    "dev": "concurrently \"yarn watch-sass\" \"yarn start\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
