    "@testing-library/user-event": "^14.5.2",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.2.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.83.0"
  }
